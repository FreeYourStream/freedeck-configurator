   enabled: true,\n  },\n  secondary: {\n    mode: EAction.noop,\n    values: {\n      [EAction.changePage]: \"\",\n      [EAction.hotkeys]: [],\n      [EAction.settings]: {},\n      [EAction.special_keys]: 0,\n      [EAction.text]: [],\n      [EAction.noop]: undefined,\n    },\n    enabled: false,\n  },\n});\nexport const createDefaultDisplay: (\n  options?: IDisplayOptions\n) => IDisplaySettings = (options) =>\n  merge<IDisplaySettings, IDisplayOptions | undefined>(\n    {\n      originalImage: null,\n      convertedImage: getEmptyConvertedImage(),\n      previewImage: getBase64Image(getEmptyConvertedImage()),\n      imageSettings: {\n        brightness: 0,\n        contrast: 0,\n        whiteThreshold: 64,\n        blackThreshold: 192,\n        dither: false,\n        invert: false,\n      },\n      textSettings: {\n        font: fontMedium,\n        text: \"\",\n        position: textPosition.right,\n      },\n      textWithIconSettings: {\n        iconWidthMultiplier: 0.35,\n      },\n      isGeneratedFromDefaultBackImage: false,\n      previousDisplay: undefined,\n      previousPage: undefined,\n    },\n    options\n  );\nexport const createDefaultDisplayButton = async (\n  displayOptions?: RecursivePartial<IDisplaySettings>\n): Promise<IDisplayButton> => {\n  return {\n    button: createDefaultButton(),\n    display: await createDefaultDisplay(displayOptions),\n  };\n};\n\nexport const createDefaultPage = async (\n  count: number,\n  previousPage?: string\n): Promise<IPage> => {\n  const page: IPage = {\n    name: \"\",\n    usePageNameAsWindowName: true,\n    isStartPage: false,\n    displayButtons: [],\n  };\n  for (var i = 0; i < count; i++) {\n    page.displayButtons.push(await createDefaultDisplayButton());\n  }\n  if (previousPage !== undefined) {\n    page.displayButtons[0].button.primary.mode = EAction.changePage;\n    page.displayButtons[0].button.primary.values[EAction.changePage] =\n      previousPage;\n    page.displayButtons[0].display = await createDefaultBackDisplay(\n      previousPage\n    );\n  }\n  return page;\n};\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/Body.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/ModalBody.tsx",["423","424"],"import React, { useEffect } from \"react\";\nimport { Route, Routes } from \"react-router-dom\";\n\nimport { CollectionSettingsModal } from \"./containers/Collection/Settings/Modal\";\nimport { DBSettingsModal } from \"./containers/DisplayButton/DisplayButtonSettingsModal\";\nimport { FDHub } from \"./containers/FDHub\";\nimport { HubPageDetails } from \"./containers/FDHub/PageDetails\";\nimport { GlobalSettings } from \"./containers/GeneralSettingsModal\";\nimport { LoginModal } from \"./containers/Login\";\nimport { PublishPage } from \"./containers/Page/Publish\";\nimport { PageSettingsModal } from \"./containers/Page/Settings/Modal\";\n\nexport const ModalBody = () => {\n  return (\n    <Routes>\n      <Route path=\"/\" element={<></>} />\n      <Route\n        path=\"/displaybutton/:pageId/:displayIndex\"\n        element={<DBSettingsModal />}\n      />\n      <Route path=\"/page/:pageId\" element={<PageSettingsModal />} />\n      <Route\n        path=\"/collection/:collectionId\"\n        element={<CollectionSettingsModal />}\n      />\n      <Route path=\"/settings\" element={<GlobalSettings />} />\n      {process.env.REACT_APP_ENABLE_API === \"true\" && (\n        <>\n          <Route path=\"/login\" element={<LoginModal />} />\n          <Route path=\"/hub/*\" element={<FDHub />} />\n          <Route path=\"/publishpage/:pageId\" element={<PublishPage />} />\n        </>\n      )}\n    </Routes>\n  );\n};\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/eventListeners.tsx",["425"],"import { CloudDownloadIcon, XIcon } from \"@heroicons/react/outline\";\nimport React from \"react\";\nimport { useEffect } from \"react\";\nimport toast from \"react-hot-toast\";\n\nimport { IAppDispatch } from \"../states/appState\";\nimport { FDButton } from \"./components/Button\";\nimport { createToast } from \"./createToast\";\n\nexport const AddEventListeners = ({\n  appDispatchContext,\n}: {\n  appDispatchContext: IAppDispatch;\n}) => {\n  const { setCtrl } = appDispatchContext;\n  return useEffect(() => {\n    window.addEventListener(\"beforeinstallprompt\", (e: Event) => {\n      e.preventDefault();\n      if (!localStorage.getItem(\"closedPWACTA\"))\n        createToast({\n          text: \"You can install the configurator to have it offline! Click here to install\",\n          danger: (t) => (\n            <FDButton\n              onClick={() => {\n                localStorage.setItem(\"closedPWACTA\", \"true\");\n                toast.dismiss(t.id);\n              }}\n              prefix={<XIcon className=\"h-4 w-4\" />}\n            >\n              Close\n            </FDButton>\n          ),\n          primary: (t) => (\n            <FDButton\n              type=\"primary\"\n              onClick={() => {\n                localStorage.setItem(\"closedPWACTA\", \"true\");\n                toast.dismiss(t.id);\n                (e as any).prompt();\n              }}\n              prefix={<CloudDownloadIcon className=\"h-4 w-4\" />}\n            >\n              Install\n            </FDButton>\n          ),\n        });\n    });\n    const onKeyUpDown = (event: KeyboardEvent) => {\n      setCtrl(event.ctrlKey);\n    };\n    document.addEventListener(\"keydown\", onKeyUpDown);\n    document.addEventListener(\"keyup\", onKeyUpDown);\n    window.onblur = () => setCtrl(false);\n    //@ts-ignore\n  }, []); // only execute on page load\n};\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/components/Title.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/serial.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/definitions/emptyConvertedImage.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/definitions/fonts.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/interfaces.ts",["426","427"],"import Joi from \"joi\";\n\nimport { EAction } from \"./definitions/modes\";\nimport { ConfigState } from \"./states/configState\";\n\nexport enum FDSettings {\n  \"change_brightness\" = 0,\n  \"absolute_brightness\",\n}\nexport interface IButtonSetting {\n  mode: EAction;\n  values: {\n    [EAction.hotkeys]: number[];\n    [EAction.changePage]: string;\n    [EAction.noop]: undefined;\n    [EAction.special_keys]: number;\n    [EAction.text]: number[];\n    [EAction.settings]: {\n      setting?: FDSettings;\n      value?: number;\n    };\n  };\n  enabled: boolean;\n}\n\nexport interface IButtonSettings {\n  primary: IButtonSetting;\n  secondary: IButtonSetting;\n}\n\nexport enum textPosition {\n  right,\n  bottom,\n}\nexport interface ITextSettings {\n  position: textPosition;\n  text: string;\n  font: string;\n}\nexport interface IImageSettings {\n  dither: boolean;\n  blackThreshold: number;\n  contrast: number;\n  brightness: number;\n  whiteThreshold: number;\n  invert: boolean;\n}\n\nexport interface ITextWithIconSettings {\n  iconWidthMultiplier: number;\n}\n\nexport type IOriginalImage = Buffer | null;\nexport type IOriginalImagePage = IOriginalImage[];\nexport type IConvertedImage = Buffer;\nexport type IConvertedImagePage = IConvertedImage[];\nexport interface IDisplaySettings {\n  imageSettings: IImageSettings;\n  textSettings: ITextSettings;\n  textWithIconSettings: ITextWithIconSettings;\n  isGeneratedFromDefaultBackImage: boolean;\n  previousPage?: string;\n  previousDisplay?: number;\n  originalImage: IOriginalImage;\n  convertedImage: IConvertedImage;\n  previewImage: string;\n}\n\nexport interface IDisplayButton {\n  display: IDisplaySettings;\n  button: IButtonSettings;\n}\n\nexport interface IPage {\n  name?: string;\n  isStartPage: boolean;\n  windowName?: string;\n  usePageNameAsWindowName: boolean;\n  isInCollection?: string;\n  displayButtons: IDisplayButton[];\n}\n\nexport type IPages = {\n  byId: { [pageId: string]: IPage };\n  sorted: string[];\n};\n\nexport interface ICollection {\n  name?: string;\n  windowName?: string;\n  usePageNameAsWindowName: boolean;\n  pages: string[];\n}\n\nexport interface ICollections {\n  byId: { [collectionId: string]: ICollection };\n  sorted: string[];\n}\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/fileToImage.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/image/base64Encode.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/image/composeImage.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/hooks/pageSwitcherHook.tsx",["428"],"import type { IpcRenderer } from \"electron\";\nimport { useEffect } from \"react\";\nimport { useLocation } from \"react-router\";\n\nimport { AppState } from \"../../states/appState\";\nimport { ConfigState } from \"../../states/configState\";\nimport { isElectron } from \"../electron\";\n\nconst findPage = (configState: ConfigState, name: string) => {\n  return configState.pages.sorted.findIndex((id) => {\n    const page = configState.pages.byId[id];\n    if (page.isInCollection) return false;\n    if (page.usePageNameAsWindowName) {\n      if (!page.name) return false;\n      return name.toLowerCase().indexOf(page.name.toLowerCase()) !== -1;\n    } else {\n      if (!page.windowName) return false;\n      return name.toLowerCase().indexOf(page.windowName.toLowerCase()) !== -1;\n    }\n  });\n};\nconst findCollectionPage = (configState: ConfigState, name: string) => {\n  return Object.values(configState.collections.byId).find((col) => {\n    if (col.usePageNameAsWindowName) {\n      if (!col.name) return false;\n      return name.toLowerCase().indexOf(col.name.toLowerCase()) !== -1;\n    } else {\n      if (!col.windowName) return false;\n      return name.toLowerCase().indexOf(col.windowName.toLowerCase()) !== -1;\n    }\n  });\n};\n\nlet lastPageIndex = -1;\nexport const usePageSwitcher = (props: {\n  configState: ConfigState;\n  appState: AppState;\n}) => {\n  const { configState, appState } = props;\n  const location = useLocation();\n  useEffect(() => {\n    if (!isElectron()) return () => {};\n    if (location.pathname !== \"/\") return () => {};\n    const ipc = (window as any).ipcRenderer as IpcRenderer;\n    const callback: (event: Electron.IpcRendererEvent, name: string) => void = (\n      event,\n      name\n    ) => {\n      (async () => {\n        // check if we have a page that looks for this window name\n        let page = findPage(configState, name);\n        // if no, look if we have a collection\n        const collection =\n          page !== -1 ? findCollectionPage(configState, name) : undefined;\n        if (page + 1 || collection) {\n          const currentPageString = await appState.serialApi?.getCurrentPage();\n          if (currentPageString === undefined) return;\n          const currentPageIndex = parseInt(currentPageString);\n          if (page + 1) {\n            if (currentPageIndex !== page)\n              appState.serialApi?.setCurrentPage(page);\n          } else if (collection) {\n            const currentId = configState.pages.sorted[currentPageIndex];\n            if (!collection.pages.includes(currentId)) {\n              const firstCollectionPage = configState.pages.sorted.findIndex(\n                (pid) => pid === collection.pages[0]\n              );\n              appState.serialApi?.setCurrentPage(firstCollectionPage);\n            }\n          }\n        }\n      })();\n    };\n    ipc.on(\"change_page\", callback);\n    return () => ipc.off(\"change_page\", callback);\n  }, [configState, appState, location]);\n};\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/GeneralSettingsModal.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Login.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Page/Publish.tsx",["429","430","431","432","433","434"],"import { userInfo } from \"os\";\nimport { config } from \"process\";\n\nimport React, { useCallback, useContext, useEffect, useState } from \"react\";\nimport { useNavigate, useParams } from \"react-router\";\n\nimport {\n  MyPagesQuery,\n  Page,\n  PageCreateMutationVariables,\n  useMeQuery,\n  usePageCreateMutation,\n} from \"../../generated/types-and-hooks\";\nimport { FDButton } from \"../../lib/components/Button\";\nimport { Label } from \"../../lib/components/LabelValue\";\nimport { Row } from \"../../lib/components/Row\";\nimport { TextInput } from \"../../lib/components/TextInput\";\nimport { FDWindow } from \"../../lib/components/Window\";\nimport { ConfigStateContext } from \"../../states/configState\";\nimport { HubPage } from \"../FDHub/components/HubPage\";\n\nexport const PublishPage: React.FC<{}> = () => {\n  const nav = useNavigate();\n  const params = useParams();\n  const { data } = useMeQuery();\n  const [mutate, { called }] = usePageCreateMutation();\n  const configState = useContext(ConfigStateContext);\n  const [name, setName] = useState(\n    configState.pages.byId[params.pageId!].name ?? \"\"\n  );\n  const [tags, setTags] = useState(\"\");\n  if (!params.pageId) return <></>;\n  const pageId = params.pageId;\n  const publishPage = async () => {\n    const result = await mutate({\n      variables: {\n        input: {\n          id: pageId,\n          data: configState.pages.byId[pageId],\n          height: configState.height,\n          width: configState.width,\n          name,\n          tags: tags\n            .split(\",\")\n            .map((t) => t.trim())\n            .filter((t) => !!t),\n          previewActions:\n            [] ??\n            configState.pages.byId[pageId].displayButtons.map(\n              (page) => page.button\n            ),\n          previewImages: configState.pages.byId[pageId].displayButtons.map(\n            (page) => page.display.previewImage\n          ),\n        },\n      },\n    });\n    if (!result.errors) nav(`/hubpage/${pageId}`);\n  };\n  const page: Page = {\n    id: pageId,\n    upvotes: -1,\n    createdBy: data?.user!,\n    height: configState.height,\n    width: configState.width,\n    name,\n    data: configState,\n    previewActions: configState.pages.byId[pageId].displayButtons.map(\n      (page) => page.button\n    ),\n    previewImages: configState.pages.byId[pageId].displayButtons.map(\n      (page) => page.display.previewImage\n    ),\n    tags: tags\n      .split(\",\")\n      .map((t) => t.trim())\n      .filter((t) => !!t),\n  };\n\n  return (\n    <FDWindow\n      title=\"Publish this page\"\n      visible={true}\n      setClose={() => nav(\"/\")}\n    >\n      <div className=\"p-10\">\n        <div className=\"mb-4 flex flex-col\">\n          <Row>\n            <Label>Name</Label>\n            <TextInput onChange={(val) => setName(val)} value={name} />\n          </Row>\n          <Row>\n            <Label>Tags</Label>\n            <TextInput onChange={(val) => setTags(val)} value={tags} />\n          </Row>\n          <Row>\n            <div className=\"flex justify-end w-full\">\n              <FDButton\n                disabled={!name || !tags.length || called}\n                onClick={publishPage}\n              >\n                Publish\n              </FDButton>\n            </div>\n          </Row>\n        </div>\n        <HubPage page={page} />\n      </div>\n    </FDWindow>\n  );\n};\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Page/Settings/Modal.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/DisplayButton/DisplayButtonSettingsModal.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Page/Pages.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/FirstTime.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Header.tsx",["435"],"import {\n  CogIcon,\n  DownloadIcon,\n  LightningBoltIcon,\n  SaveIcon,\n  UploadIcon,\n} from \"@heroicons/react/outline\";\nimport c from \"clsx\";\nimport React, { useContext, useEffect, useRef, useState } from \"react\";\nimport { Link, useNavigate } from \"react-router-dom\";\n\nimport { iconSize } from \"../definitions/iconSizes\";\nimport { FDButton } from \"../lib/components/Button\";\nimport { Value } from \"../lib/components/LabelValue\";\nimport { createConfigBuffer } from \"../lib/configFile/createBuffer\";\nimport { loadConfigFile } from \"../lib/configFile/loadConfigFile\";\nimport { download } from \"../lib/download\";\nimport { connectionStatus } from \"../lib/serial\";\nimport { AppStateContext } from \"../states/appState\";\nimport {\n  ConfigDispatchContext,\n  ConfigStateContext,\n} from \"../states/configState\";\nimport { LoginLogoutButtons } from \"./LoginButton\";\n\nexport const Header: React.FC<{}> = () => {\n  const configState = useContext(ConfigStateContext);\n  const { pages } = configState;\n  const { setState } = useContext(ConfigDispatchContext);\n  const { serialApi, ctrlDown } = useContext(AppStateContext);\n  const nav = useNavigate();\n  const [connected, setConnected] = useState<boolean>(!!serialApi?.connected);\n  const [progress, setProgress] = useState<number>(0);\n  const loadConfigRef = useRef<HTMLInputElement | null>(null);\n  useEffect(() => {\n    if (!serialApi) return;\n    const id = serialApi.registerOnConStatusChange((type) => {\n      setConnected(type === connectionStatus.connect);\n    });\n\n    return () => serialApi.clearOnConStatusChange(id);\n  }, [serialApi]);\n\n  const saveConfigFile = () => {\n    if (Object.keys(pages.byId).length === 0) return;\n    const completeBuffer = createConfigBuffer(configState);\n\n    completeBuffer && download(completeBuffer);\n  };\n  return (\n    <div\n      id=\"header\"\n      className={c(\n        \"border-b-2 border-gray-600 bg-gray-700 flex h-24 w-full items-center px-10 shadow-2xl\"\n      )}\n    >\n      <div className={c(\"flex mr-12\")}>\n        <div className={c(\"font-thin text-5xl text-white font-fd\")}>Free</div>\n        {/*\"font-thin text-5xl text-white \"/*/}\n        {/*\"font-thin text-5xl bg-gradient-to-br from-red-100 to-gray-400 rounded-sm\"*/}\n        <div className={c(\"font-medium text-5xl text-white font-fd\")}>Deck</div>\n      </div>\n      <div className={c(\"flex h-14 justify-between items-center w-full\")}>\n        <form\n          className=\"flex items-center space-x-4\"\n          onSubmit={(event) => {\n            event.preventDefault();\n          }}\n        >\n          <input\n            title=\"loadConfig\"\n            className={c(\"hidden\")}\n            type=\"file\"\n            id=\"loadConfig\"\n            ref={loadConfigRef}\n            onChange={(event) => {\n              event.currentTarget.files &&\n                loadConfigFile(event.currentTarget.files, setState);\n            }}\n          ></input>\n          <div\n            className={c(\"flex items-center space-x-4 h-auto overflow-hidden\")}\n          >\n            {connected && !ctrlDown ? (\n              <>\n                <FDButton\n                  prefix={<UploadIcon className={iconSize} />}\n                  size={3}\n                  onClick={() =>\n                    serialApi!\n                      .readConfigFromSerial((rec, size) =>\n                        setProgress(rec / size)\n                      )\n                      .then((data) => loadConfigFile(data, setState))\n                  }\n                >\n                  Load from FreeDeck\n                </FDButton>\n                <FDButton\n                  prefix={<SaveIcon className={iconSize} />}\n                  size={3}\n                  onClick={async () =>\n                    serialApi!.writeConfigOverSerial(\n                      createConfigBuffer(configState),\n                      (rec, size) => setProgress(rec / size)\n                    )\n                  }\n                >\n                  Save to FreeDeck\n                </FDButton>\n                <Value className=\"ml-4\">\n                  {progress ? `${(progress * 100).toFixed(0)}%` : \"\"}\n                </Value>\n              </>\n            ) : (\n              <>\n                <FDButton\n                  prefix={<UploadIcon className={iconSize} />}\n                  size={3}\n                  onClick={() => {\n                    loadConfigRef.current?.click();\n                  }}\n                >\n                  Load Config\n                </FDButton>\n\n                {!!Object.keys(pages).length && (\n                  <FDButton\n                    prefix={<DownloadIcon className={iconSize} />}\n                    disabled={!Object.keys(pages).length}\n                    size={3}\n                    onClick={() => saveConfigFile()}\n                  >\n                    Save Config\n                  </FDButton>\n                )}\n                {serialApi && !serialApi.connected && (\n                  <FDButton\n                    prefix={<LightningBoltIcon className={iconSize} />}\n                    size={3}\n                    onClick={() =>\n                      serialApi.connect().catch((e) => console.log(e))\n                    }\n                  >\n                    Connect to FreeDeck\n                  </FDButton>\n                )}\n              </>\n            )}\n          </div>\n        </form>\n        <div className={c(\"flex items-center space-x-4\")}>\n          <FDButton\n            prefix={<CogIcon className={iconSize} />}\n            onClick={() => nav(\"/settings\")}\n            size={3}\n          >\n            Settings\n          </FDButton>\n\n          {process.env.REACT_APP_ENABLE_API === \"true\" && (\n            <LoginLogoutButtons\n              openLogin={() => nav(\"/login\")}\n              openFDHub={() => nav(\"/hub/home\")}\n            />\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/ContentBody.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Collection/Collections.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Collection/Settings/Modal.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/definitions/headers.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/image/colorToMonoBitmap.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/handleFileSelect.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/FDHub/index.tsx",["436","437","438"],"import { HomeIcon, SearchIcon } from \"@heroicons/react/outline\";\nimport React, { useState } from \"react\";\nimport {\n  Route,\n  Routes,\n  useLocation,\n  useNavigate,\n  useParams,\n} from \"react-router\";\n\nimport { iconSize } from \"../../definitions/iconSizes\";\nimport { TabView } from \"../../lib/components/TabView\";\nimport { FDWindow } from \"../../lib/components/Window\";\nimport { Home } from \"./Home\";\nimport { HubPageDetails } from \"./PageDetails\";\nimport { FDSearch } from \"./Search\";\n\nexport const FDHub = () => {\n  const nav = useNavigate();\n\n  return (\n    <FDWindow\n      className=\"h-full w-full\"\n      title=\"FreeDeck Hub\"\n      visible={true}\n      setClose={() => nav(\"/\")}\n    >\n      <TabView\n        className=\"h-full\"\n        tabs={[\n          {\n            title: \"Home\",\n            prefix: <HomeIcon className={iconSize} />,\n            content: <Home />,\n          },\n          {\n            title: \"Search\",\n            prefix: <SearchIcon className={iconSize} />,\n            content: <FDSearch />,\n          },\n        ]}\n      />\n      <Routes>\n        <Route path=\"page/:pageId\" element={<HubPageDetails />} />\n      </Routes>\n    </FDWindow>\n  );\n};\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/util.ts",["439"],"import { useContext } from \"react\";\n\nimport { ICollection, IPage } from \"../interfaces\";\n\nexport const getPageName = (pageId: string, page?: IPage) => {\n  return page?.name ? page.name : pageId.slice(-4);\n};\n\nexport const getCollectionName = (\n  collectionId: string,\n  collection?: ICollection\n) => {\n  return collection?.name ? collection.name : collectionId.slice(-4);\n};\n","/home/koriwi/Development/Hobby/freedeck-configurator/web/src/definitions/iconSizes.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Page/Settings/General.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Page/Settings/AutoPageSwitcher.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/components/Window.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/components/TabView.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/generated/types-and-hooks.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/download.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/FDHub/components/HubPage.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/configFile/loadConfigFile.ts",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/components/Row.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/components/TextInput.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/lib/components/LabelValue.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Displays.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Serial.tsx",[],"/home/koriwi/Development/Hobby/freedeck-configurator/web/src/containers/Device.tsx",[],"/home/koriwi/Development/Hobby/f