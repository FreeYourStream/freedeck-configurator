    },
    "windows": [
      {
        "fullscreen": false,
        "height": 768,
        "minHeight": 768,
        "resizable": true,
        "title": "FreeDeck Configurator",
        "width": 1366,
        "minWidth": 1366,
        "fileDropEnabled": false
      }
    ]
  }
