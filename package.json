{
  "name": "freedeck-configurator",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "web",
    "app"
  ],
  "scripts": {
    "build:web": "npm run build -w web",
    "build:app": "npm run build -w app",
    "dev:web": "npm run start -w web",
    "dev:app": "npm run dev -w app",
    "dev": "concurrently \"npm run dev:web\" \"npm run dev:app\"",
    "prebuild": "npm run build:web && npm run build:app && cp -r web/build app/build",
    "build": "npx electron-builder --project app"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/koriwi/freedeck-configurator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/koriwi/freedeck-configurator/issues"
  },
  "homepage": "https://github.com/koriwi/freedeck-configurator#readme",
  "devDependencies": {
    "concurrently": "^6.4.0"
  }
}
